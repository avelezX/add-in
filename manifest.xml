<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<OfficeApp
  
  xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
  xsi:type="TaskPaneApp">

  <!-- Este ES el ID que identifica tu add-in. Puedes mantenerlo,
       o cambiarlo por un GUID nuevo si Excel sigue mostrando el popup del add-in viejo. -->
  <Id>c2f3a892-d0b6-4d67-bc84-7e10eaefc0b7</Id>

  <Version>1.0.0.2</Version>
  <ProviderName>Saman WM</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Saman-xerenity"/>
  <Description DefaultValue="Custom Functions that read Xerenity data"/>
  <IconUrl DefaultValue="https://avelezx.github.io/add-in/assets/logo-filled.png"/>
  <HighResolutionIconUrl DefaultValue="https://avelezx.github.io/add-in/assets/logo-filled.png"/>

  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="CustomFunctionsRuntime" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://avelezx.github.io/add-in/taskpane.html"/>
  </DefaultSettings>

  <Permissions>ReadWriteDocument</Permissions>

  <!-- IMPORTANTE: VersionOverrides V1_1 con el schema de *mailappversionoverrides/1.1* -->
  <VersionOverrides
    xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides/1.0"
    xsi:type="VersionOverridesV1_0">

    <Hosts>
      <Host xsi:type="Workbook">
        <AllFormFactors>
          <ExtensionPoint xsi:type="CustomFunctions">
            <!-- El <Id> dentro de ExtensionPoint es opcional; lo omito -->
            <Script>https://avelezx.github.io/add-in/assets/functions.js</Script>
            <Metadata>https://avelezx.github.io/add-in/assets/custom-functions.json</Metadata>
            <Page>https://avelezx.github.io/add-in/taskpane.html</Page>
            <Namespace>XERENITY</Namespace>
          </ExtensionPoint>
        </AllFormFactors>
      </Host>
    </Hosts>

    <!-- Recursos opcionales; quitÃ© las entradas /public que no existen -->
    <Resources>
      <bt:Images>
  <bt:Image id="Icon.16x16" DefaultValue="https://avelezx.github.io/add-in/assets/logo-filled.png"/>
  <bt:Image id="Icon.32x32" DefaultValue="https://avelezx.github.io/add-in/assets/logo-filled.png"/>
  <bt:Image id="Icon.80x80" DefaultValue="https://avelezx.github.io/add-in/assets/logo-filled.png"/>
      </bt:Images>
      <bt:Urls>
  <bt:Url id="Taskpane.Url" DefaultValue="https://avelezx.github.io/add-in/taskpane.html"/>
  <bt:Url id="Commands.Url" DefaultValue="https://avelezx.github.io/add-in/commands.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="Functions.Namespace" DefaultValue="XERENITY"/>
        <bt:String id="GetStarted.Title" DefaultValue="Saman-xerenity"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Saman"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Open"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Custom Functions for Xerenity."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Open task pane"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
  <SupportUrl DefaultValue="https://avelezx.github.io/add-in/support"/>
</OfficeApp>
