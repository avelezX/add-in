<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
           xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides/1.0"
           xsi:type="TaskPaneApp">

  <Id>0fb3d32f-7a0b-4e26-944c-4e8a8ffca01a</Id>
  <Version>1.0.0.8</Version>
  <ProviderName>Saman WM</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>

  <DisplayName DefaultValue="Saman Xerenity Functions"/>
  <Description DefaultValue="Custom functions that read Xerenity data"/>

  <IconUrl DefaultValue="https://avelezx.github.io/add-in/assets/logo-filled.png"/>
  <HighResolutionIconUrl DefaultValue="https://avelezx.github.io/add-in/assets/logo-filled.png"/>

  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="ExcelApi" MinVersion="1.13"/>
    </Sets>
  </Requirements>

  <Permissions>ReadWriteDocument</Permissions>

  <!-- Esta pÃ¡gina invisible carga el runtime de las funciones -->
  <DefaultSettings>
    <SourceLocation DefaultValue="https://avelezx.github.io/add-in/assets/functions.html"/>
  </DefaultSettings>

  <ov:VersionOverrides xsi:type="VersionOverridesV1_0">
    <ov:Hosts>
      <ov:Host xsi:type="Workbook">
        <ov:DesktopFormFactor>
          <ov:ExtensionPoint xsi:type="TaskPane">
            <ov:SourceLocation resid="Functions.Page.Url"/>
          </ov:ExtensionPoint>
        </ov:DesktopFormFactor>
      </ov:Host>
    </ov:Hosts>

    <ov:Resources>
      <bt:Urls>
        <bt:Url id="Functions.Page.Url" DefaultValue="https://avelezx.github.io/add-in/assets/functions.html"/>
      </bt:Urls>
    </ov:Resources>
  </ov:VersionOverrides>
</OfficeApp>