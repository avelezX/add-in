<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
           xsi:type="TaskPaneApp">

  <Id>c2f3a892-d0b6-4d67-bc84-7e10eaefc0b7</Id>
  <Version>1.0.0.5</Version>
  <ProviderName>Saman WM</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>

  <DisplayName DefaultValue="Saman-xerenity"/>
  <Description DefaultValue="Custom Functions that read Xerenity data"/>

  <IconUrl DefaultValue="https://avelezx.github.io/add-in/assets/logo-filled.png"/>
  <HighResolutionIconUrl DefaultValue="https://avelezx.github.io/add-in/assets/logo-filled.png"/>

  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="CustomFunctionsRuntime" MinVersion="1.1"/>
    </Sets>
  </Requirements>

  <Permissions>ReadWriteDocument</Permissions>

  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides"
                    xsi:type="VersionOverridesV1_1">

    <Hosts>
      <Host xsi:type="Workbook">
        <AllFormFactors>
          <ExtensionPoint xsi:type="CustomFunctions">
            <Script>
              <SourceLocation resid="Functions.Script.Url"/>
            </Script>
            <Metadata>
              <SourceLocation resid="Functions.Metadata.Url"/>
              <Namespace resid="Functions.Namespace"/>
            </Metadata>
          </ExtensionPoint>
        </AllFormFactors>
      </Host>
    </Hosts>

    <Resources>
      <bt:Urls>
        <bt:Url id="Functions.Script.Url" DefaultValue="https://avelezx.github.io/add-in/assets/functions.js"/>
        <bt:Url id="Functions.Metadata.Url" DefaultValue="https://avelezx.github.io/add-in/assets/custom-functions.json"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="Functions.Namespace" DefaultValue="XERENITY"/>
      </bt:ShortStrings>
    </Resources>

  </VersionOverrides>

  <SupportUrl DefaultValue="https://avelezx.github.io/add-in/support"/>

</OfficeApp>